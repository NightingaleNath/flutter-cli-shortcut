{
  "name": "flutter-cli-shortcut",
  "displayName": "Flutter CLI Shortcut",
  "description": "A collection of useful Flutter CLI commands for quick access.",
  "version": "0.1.0",
  "engines": {
    "vscode": "^1.94.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "activationEvents": [
    "onLanguage:dart"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "flutter-helper.flutterRun",
        "title": "Flutter: Run App"
      },
      {
        "command": "flutter-helper.flutterPubGet",
        "title": "Flutter: Pub Get"
      },
      {
        "command": "flutter-helper.flutterPubUpgrade",
        "title": "Flutter: Pub Upgrade"
      },
      {
        "command": "flutter-helper.flutterClean",
        "title": "Flutter: Clean Project"
      },
      {
        "command": "flutter-helper.flutterTest",
        "title": "Flutter: Test"
      },
      {
        "command": "flutter-helper.flutterAnalyze",
        "title": "Flutter: Analyze"
      },
      {
        "command": "flutter-helper.flutterCreate",
        "title": "Flutter: Create Project"
      },
      {
        "command": "flutter-helper.flutterRunRelease",
        "title": "Flutter: Run Release"
      },
      {
        "command": "flutter-helper.flutterBuildApk",
        "title": "Flutter: Build APK"
      },
      {
        "command": "flutter-helper.flutterBuildIos",
        "title": "Flutter: Build iOS"
      },
      {
        "command": "flutter-helper.flutterBuildAppBundle",
        "title": "Flutter: Build App Bundle"
      },
      {
        "command": "flutter-helper.podInstall",
        "title": "iOS: Pod Install"
      },
      {
        "command": "flutter-helper.podInstallWithRepoUpdate",
        "title": "iOS: Pod Install with Repo Update"
      },
      {
        "command": "flutter-helper.podUpdate",
        "title": "iOS: Pod Update"
      },
      {
        "command": "flutter-helper.podRepoUpdate",
        "title": "iOS: Pod Repo Update"
      },
      {
        "command": "flutter-helper.flutterPubGlobalActivate",
        "title": "Flutter: Pub Global Activate"
      },
      {
        "command": "flutter-helper.flutterChannel",
        "title": "Flutter: Change Channel"
      },
      {
        "command": "flutter-helper.flutterVersion",
        "title": "Flutter: Current Version"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "webpack",
    "watch": "webpack --watch",
    "package": "webpack --mode production --devtool hidden-source-map",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "npm run compile-tests && npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.8",
    "@types/node": "20.x",
    "@types/vscode": "^1.94.0",
    "@typescript-eslint/eslint-plugin": "^8.7.0",
    "@typescript-eslint/parser": "^8.7.0",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1",
    "eslint": "^9.11.1",
    "ts-loader": "^9.5.1",
    "typescript": "^5.6.2",
    "webpack": "^5.95.0",
    "webpack-cli": "^5.1.4"
  },
  "publisher": "NightingaleNath",
  "repository": {
    "type": "git",
    "url": "https://github.com/NightingaleNath/flutter-cli-shortcut.git" 
  },
  "bugs": {
    "url": "https://github.com/NightingaleNath/flutter-cli-shortcut/issues"
  },
  "homepage": "https://github.com/NightingaleNath/flutter-cli-shortcut#readme"
}
